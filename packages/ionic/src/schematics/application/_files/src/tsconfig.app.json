{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "baseUrl": "./",
    "paths": {
      "@<%= npmScope %>/*": ["../../../libs/*"],
      "@<%= npmScope %>/ionic": ["../../../xplat/ionic/index.ts"],
